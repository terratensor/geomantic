# Manticore
MANTICORE_HOST=localhost
MANTICORE_PORT=9308
MANTICORE_TIMEOUT=30s

# Download
GEONAMES_BASE_URL=https://download.geonames.org/export/dump/
DOWNLOAD_TIMEOUT=10m
MAX_RETRIES=3

# Pipeline
BATCH_SIZE=20000
WORKERS_COUNT=16
CHANNEL_BUFFER_SIZE=500000

# Import
DATA_DIR=./data
FULL_IMPORT=true
UPDATE_ENABLED=false

# S2 Geometry
S2_GEOHASH_LEVEL=9  # уровень точности геохеша (1-30, рекомендуется 8-10)

# Language filters for name dictionary
EXCLUDE_CJK=false     # исключать китайские, японские, корейские иероглифы
EXCLUDE_ARABIC=false  # исключать арабские символы